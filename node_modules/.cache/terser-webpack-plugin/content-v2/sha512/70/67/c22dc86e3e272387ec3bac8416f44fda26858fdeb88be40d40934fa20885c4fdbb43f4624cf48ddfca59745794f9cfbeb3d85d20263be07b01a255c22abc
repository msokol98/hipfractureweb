={idx} className={classes} dangerouslySetInnerHTML={createMarkup(answerChoice.body)} />\n                \n        \n            })}\n        </div>\n    )\n}\n \nexport default Question;","const mapIdxToLetter = idx => {\n    switch(idx) {\n        case 0: return 'A';\n        case 1: return 'B';\n        case 2: return 'C';\n        case 3: return 'D';\n        default: return 'D';\n    }\n}\n\nexport default mapIdxToLetter;","import React from 'react';\nimport './styles.css';\nimport mapIdxToLetter from 'utils/mapIdxToLetter';\n\nconst AnswerChoice = ({ value, idx, remove, onChange, placeholder, correctAnswer, setCorrectAnswer }) => {\n\n    let classes = \"input with-space-on-right\";\n\n    if(correctAnswer === mapIdxToLetter(idx)) {\n        classes += \" correct-answer\";\n    }\n\n    return(\n        <div className=\"answer-choice\">\n            <input \n                required\n                type=\"text\" \n                placeholder={placeholder}\n                value={value}\n                onChange={onChange}\n                name={idx}\n                className={classes}\n            />\n\n            <input type=\"radio\" onClick={() => setCorrectAnswer(idx)} name=\"answer-choices\" className=\"with-space-on-right\" />\n\n            <button onClick={() => remove(idx)} className=\"delete\">Remove</button>\n        </div>\n    );\n\n};\n \nexport default AnswerChoice;","import React, { Component } from 'react';\nimport AnswerChoice from './answerChoice';\nimport mapIdxToLetter from 'utils/mapIdxToLetter';\nimport './styles.css';\n\nclass NewQuestion extends Component {\n\n    state = {\n        prompt: \"\",\n        answerChoices: [\n            {body: \"\"},\n            {body: \"\"},\n            {body: \"\"},\n            {body: \"\"}\n        ],\n        correctAnswer: null\n    }\n\n    removeAnswer = deletionIdx => {\n        const remainingAnswers = this.state.answerChoices.filter((_, idx) => idx !== deletionIdx);\n\n        if(remainin